{
  "version": 2,
  "routes": [
    {
      "src": "/api/bot/webhook",
      "dest": "/api/bot/webhook.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/backend/index.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/backend/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
